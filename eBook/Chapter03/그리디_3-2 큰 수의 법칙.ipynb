   "source": [
    "n, m, k = map(int, input().split())\n",
    "\n",
    "data = list(map(int, input().split()))\n",
    "\n",
    "data.sort()\n",
    "\n",
    "less = m // k\n",
    "more = m - less\n",
    "\n",
    "total = data[n - 1] * more + data[n - 2] * less\n",
    "\n",
    "print(total)"
   ]
